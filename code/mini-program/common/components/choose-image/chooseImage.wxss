.n-ch {
  border: 1px solid #d6dbe2;
  position: relative;
}
.n-ch::before {
  background: #d6dbe2;
  height: 1px;
  top: 50%;
  left: 20%;
  width: 60%;
  position: absolute;
  content: '';
}
.n-ch::after {
  background: #d6dbe2;
  width: 1px;
  top: 20%;
  left: 50%;
  height: 60%;
  position: absolute;
  content: '';
}

.p-ch {
  border: 1px solid #d6dbe2;
  position: relative;
}
